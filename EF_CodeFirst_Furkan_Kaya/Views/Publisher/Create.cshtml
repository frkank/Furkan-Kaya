@model EF_CodeFirst.Models.Publisher
@{
    ViewData["Title"] = "Create";
}
<h4>New Publisher</h4>
<div class="row">
    <div class="col-sm-6">
        <form asp-action="create" class="col-form-label">
            <input type="text" asp-for="PublisherName" placeholder="Publisher Name..." class="form-control mb-4">
            <input type="text" asp-for="PublisherCity" placeholder="Publisher City..." class="form-control mb-4">
            <input type="text" asp-for="PublisherMail" placeholder="Publisher Mail..." class="form-control mb-4">
            <input type="submit" value="Create" class="btn btn-success">
        </form>
        <a asp-action="Index" class="btn btn-info">Back to Publisher List</a>
    </div>
</div>